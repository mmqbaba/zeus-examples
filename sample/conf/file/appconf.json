{
    "go_micro": {
        "service_name": "sample",
        "registry_plugin_type": "",
        "registry_addrs": ["127.0.0.1:2379"]
    },
    "mongodb": {
        "enable": false
    },
    "http_client": {
        "test": {
            "instancename": "test",
            "host_name": "http://ip-api.com"
        }
    },
    "redis": {
        "host": "127.0.0.1:6379",
        "poolsize": 50,
        "conn_idle_timeout": 10,
        "enable": false
    },
    "log_conf": {
        "log": "console",
        "level": "debug",
        "format": "text",
        "disable_report_caller": false,
        "rotation_time": "hour",
        "log_dir": "./"
    },
    "accesslog": {
        "conf": {
            "log": "file",
            "level": "info",
            "format": "text",
            "disable_report_caller": true,
            "rotation_time": "hour",
            "log_dir": "./accesslog"
        },
        "enable_recorded": true
    },
    "trace":{
        "server_name":"sample",
        "trace_url":"http://127.0.0.1:9411/api/v1/spans",
        "only_log_err":false
    },
    "gops": {
        "enable": true,
        "addr": ":19999"
    }
}
